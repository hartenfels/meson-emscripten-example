[binaries]
c = 'emcc'
cpp = 'em++'
ar = 'emar'
pkgconfig = ['emmake', 'env', 'PKG_CONFIG_PATH=PREFIX_GOES_HERE/lib/pkgconfig', 'pkg-config']
cmake = ['emmake', 'cmake']
sdl2-config = ['emconfigure', 'sdl2-config']
exe_wrapper = 'node'

[properties]
needs_exe_wrapper = true

[host_machine]
system = 'wasm'
cpu_family = 'wasm'
cpu = 'wasm'
endian = 'little'
